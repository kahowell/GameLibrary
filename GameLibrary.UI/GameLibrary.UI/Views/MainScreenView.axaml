<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:GameLibrary.UI.ViewModels"
             xmlns:rxui="http://reactiveui.net"
             xmlns:ui="clr-namespace:GameLibrary.UI"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GameLibrary.UI.Views.MainScreenView"
             x:DataType="viewModels:AppViewModel">
  <rxui:RoutedViewHost Router="{Binding Router}">
    <rxui:RoutedViewHost.DefaultContent>
      Screen not found. Please file an issue.
    </rxui:RoutedViewHost.DefaultContent>
    <rxui:RoutedViewHost.ViewLocator>
      <ui:ReactiveViewLocator />
    </rxui:RoutedViewHost.ViewLocator>
  </rxui:RoutedViewHost>
</UserControl>
